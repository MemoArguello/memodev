---
import ProyectosItem from "./ProyectosItem.astro"

const PROYECTOS = [
    {
        title: "Repositorio Web",
        descripcion: "repositorio para una facultad",
        link: "#",
        date: "2024",
    }
]
---
<div class="max-w-sm bg-white border border-gray-200 rounded-lg shadow dark:bg-gray-800 dark:border-gray-700">
    {
        PROYECTOS.map(experiencia => (
            <li class="mb-10 ms-4">
                <ProyectosItem {... experiencia}/>
            </li>
        ))
    }
</div>